{
  "python.linting.flake8Enabled": true,
  "python.linting.mypyEnabled": true,
  "python.linting.enabled": true,

  "editor.rulers": [
      99
  ],
  "search.exclude": {
      "# Byte-compiled / optimized / DLL files": true,
      "**/__pycache__": true,
      "*.py[cod]": true,
      "*$py.class": true,
      "# C extensions": true,
      "*.so": true,
      "# Distribution / packaging": true,
      ".Python": true,
      "build/": true,
      "develop-eggs/": true,
      "dist/": true,
      "downloads/": true,
      "eggs/": true,
      ".eggs/": true,
      "lib/": true,
      "lib64/": true,
      "parts/": true,
      "sdist/": true,
      "var/": true,
      "wheels/": true,
      "pip-wheel-metadata/": true,
      "share/python-wheels/": true,
      "*.egg-info/": true,
      ".installed.cfg": true,
      "*.egg": true,
      "MANIFEST": true,
      "# PyInstaller": true,
      "#  Usually these files are written by a python script from a template": true,
      "#  before PyInstaller builds the exe, so as to inject date/other infos into it.": true,
      "*.manifest": true,
      "*.spec": true,
      "# Installer logs": true,
      "pip-log.txt": true,
      "pip-delete-this-directory.txt": true,
      "# Unit test / coverage reports": true,
      "htmlcov/": true,
      ".tox/": true,
      ".nox/": true,
      ".coverage": true,
      ".coverage.*": true,
      ".cache": true,
      "nosetests.xml": true,
      "coverage.xml": true,
      "*.cover": true,
      "*.py,cover": true,
      ".hypothesis/": true,
      ".pytest_cache/": true,
      "# Translations": true,
      "*.mo": true,
      "*.pot": true,
      "# Django stuff:": true,
      "*.log": true,
      "local_settings.py": true,
      "db.sqlite3": true,
      "db.sqlite3-journal": true,
      "# Flask stuff:": true,
      "instance/": true,
      ".webassets-cache": true,
      "# Scrapy stuff:": true,
      ".scrapy": true,
      "# Sphinx documentation": true,
      "docs/_build/": true,
      "# PyBuilder": true,
      "target/": true,
      "# Jupyter Notebook": true,
      ".ipynb_checkpoints": true,
      "# IPython": true,
      "profile_default/": true,
      "ipython_config.py": true,
      "# pipenv": true,
      "#   According to pypa/pipenv#598, it is recommended to include Pipfile.lock in version control.": true,
      "#   However, in case of collaboration, if having platform-specific dependencies or dependencies": true,
      "#   having no cross-platform support, pipenv may install dependencies that don't work, or not": true,
      "#   install all needed dependencies.": true,
      "#Pipfile.lock": true,
      "# PEP 582; used by e.g. github.com/David-OConnor/pyflow": true,
      "__pypackages__/": true,
      "# Celery stuff": true,
      "celerybeat-schedule": true,
      "celerybeat.pid": true,
      "# SageMath parsed files": true,
      "*.sage.py": true,
      "# Environments": true,
      ".env": false,
      ".venv": true,
      "env/": true,
      "venv/": true,
      "ENV/": true,
      "env.bak/": true,
      "venv.bak/": true,
      "# Spyder project settings": true,
      ".spyderproject": true,
      ".spyproject": true,
      "# Rope project settings": true,
      ".ropeproject": true,
      "# mkdocs documentation": true,
      "/site": true,
      "# mypy": true,
      ".mypy_cache/": true,
      ".dmypy.json": true,
      "dmypy.json": true,
      "# Pyre type checker": true,
      ".pyre/": true
  },
